<?xml version="1.0" ?>
<settings version="1">
	<section id="plugin.video.xstream">
		<category id="general" label="30020" help="">		
			<group id="1" label="30030">
				<setting id="DevUpdateAuto" type="boolean" label="30096" help="">
					<level>0</level>
					<default>False</default>
					<dependencies>
						<dependency type="enable">
							<or>
								<condition operator="is" setting="enforceUpdate">False</condition>
							</or>
						</dependency>
					</dependencies>					
					<control type="toggle"/>
				</setting>
				<setting id="githubUpdateXstream" type="boolean" label="30010" help="30097">
					<level>0</level>
					<default>False</default>
					<dependencies>
						<dependency type="enable">
							<or>
								<condition operator="is" setting="DevUpdateAuto">True</condition>
							</or>
						</dependency>
					</dependencies>
					<control type="toggle"/>					
				</setting>
				<setting id="githubUpdateResolver" type="boolean" label="30015" help="">
					<level>0</level>
					<default>False</default>
					<dependencies>
						<dependency type="enable">
							<or>
								<condition operator="is" setting="DevUpdateAuto">True</condition>
							</or>
						</dependency>
					</dependencies>
					<control type="toggle"/>					
				</setting>
				<setting id="resolver.branch" type="string" label="30098" help="30101">
					<level>0</level>
					<default>release</default>
					<constraints>
						<options>
							<option label="30099">release</option>
							<option label="30100">nightly</option>
						</options>
					</constraints>
					<dependencies>
						<dependency type="enable">
							<and>
								<condition operator="is" setting="githubUpdateResolver">True</condition>
								<condition operator="is" setting="DevUpdateAuto">True</condition>
							</and>
						</dependency>
					</dependencies>					
					<control type="spinner" format="string"/>
				</setting>
				<setting id="enforceUpdate" type="boolean" label="30102" help="">
					<level>0</level>
					<default>False</default>
					<dependencies>
						<dependency type="enable">
							<or>
								<condition operator="is" setting="DevUpdateAuto">False</condition>
							</or>
						</dependency>
					</dependencies>
					<control type="toggle"/>					
				</setting>	
				<setting id="newSetting" type="boolean" help="">
					<level>0</level>
					<default>True</default>
					<dependencies>
						<dependency type="visible">
							<condition on="property" name="InfoBool">false</condition>
						</dependency>
					</dependencies>
					<control type="toggle"/>
				</setting>
			</group>		
